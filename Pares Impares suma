#Leemos 5 numeros enteros
#sumamos los pares por un lado
#sumamos los impares por otro 
def suma_par_impar():
    suma_par=0
    suma_impar=0
    for cont in range(1,6):
        num=int(input("Dame un numero: "))
        if(num%2==0):
            print("ES PAR")#Rama del verdadero
            suma_par=suma_par+num
        else:
            print("ES IMPAR")#Rama del falso
            suma_impar=suma_impar+num
    print("La suma de los pares es= "+str(suma_par))
    print("La suma de los impares es= "+str(suma_impar))
    
suma_par_impar()
